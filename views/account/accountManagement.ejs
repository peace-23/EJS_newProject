<div>
    <h1>
        <%= title %>
    </h1>

    <!-- Account Management View Message -->
    <%- messages() %>

        <!-- Account Management View Error Display -->
        <% if (errors) { %>
            <ul class="notice">
                <% errors.array().forEach(error=> { %>
                    <li>
                        <%= error.msg %>
                    </li>
                    <% }) %>
            </ul>
            <% } %>

                <!-- Vehicle Management View Display -->
                <h2>Welcome <%= locals.accountData.account_firstname %>
                </h2>

                <p>You're logged in</p>

                <p><a href="/account/update/<%- locals.accountData.account_id %>">Update Account
                        information</a></p>
                <% if (locals.accountData.account_type==="Admin" || locals.accountData.account_type==="Employee" ) { %>
                    <p><a href="/account/delete-confirm/<%- locals.accountData.account_id %>">Delete
                            Account</a></p>

                    <h3>Inventory Management</h3>
                    <p><a href="/inv">Inventory Management</a></p>
                    <% } %>




</div>